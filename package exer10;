package exer10;

public interface Matematica{
	interger operar(Interger v1, Interger v2);
}

package exer10;

import org.junit.Test;

public class Exer10{
	
	public void calcular(Matematica m){
	System.out.println(m.opera (10,10));

	}

	@Test
	public void exer10(){
	calcular((v1,v2) -> v1 + v2)
	calcular((v1,v2) -> v1 - v2)

	}
}